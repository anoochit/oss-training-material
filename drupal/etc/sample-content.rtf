\uc0\u3652 \u3617 \u3650 \u3588 \u3619  \u3595 \u3629 \u3615 \u3607 \u3660 \u3619 \u3632 \u3610 \u3640 \u3623 \u3656 \u3634  \u3588 \u3629 \u3617 \u3614 \u3660 \u3650 \u3605 \u3658 \u3632 \u3585 \u3634 \u3649 \u3615 \u3609 \u3637 \u3657 \u3617 \u3637 \u3588 \u3623 \u3634 \u3617 \u3626 \u3634 \u3617 \u3634 \u3619 \u3606 \u3604 \u3657 \u3634 \u3609  "Multi-touch" \u3627 \u3619 \u3639 \u3629 \u3588 \u3623 \u3634 \u3617 \u3626 \u3634 \u3617 \u3634 \u3619 \u3606 \u3651 \u3609 \u3585 \u3634 \u3619 \u3623 \u3636 \u3648 \u3588 \u3619 \u3634 \u3632 \u3627 \u3660 \u3629 \u3636 \u3609 \u3614 \u3640 \u3607 \u3607 \u3637 \u3656 \u3648 \u3585 \u3636 \u3604 \u3592 \u3634 \u3585 \u3585 \u3634 \u3619 \u3626 \u3633 \u3617 \u3612 \u3633 \u3626 \u3627 \u3609 \u3657 \u3634 \u3592 \u3629 \u3586 \u3629 \u3591 \u3612 \u3641 \u3657 \u3651 \u3594 \u3657 \u3627 \u3621 \u3634 \u3618  \u3588 \u3609  \u3648 \u3607 \u3588 \u3650 \u3609 \u3650 \u3621 \u3618 \u3637 \u3604 \u3633 \u3591 \u3585 \u3621 \u3656 \u3634 \u3623 \u3617 \u3637 \u3621 \u3633 \u3585 \u3625 \u3603 \u3632 \u3649 \u3609 \u3623 \u3588 \u3636 \u3604 \u3649 \u3621 \u3632 \u3594 \u3639 \u3656 \u3629 \u3648 \u3604 \u3637 \u3618 \u3623 \u3585 \u3633 \u3610 \u3648 \u3607 \u3588 \u3650 \u3609 \u3650 \u3621 \u3618 \u3637  "MultiTouch" \u3595 \u3638 \u3656 \u3591 \u3626 \u3605 \u3637 \u3615  \u3592 \u3655 \u3629 \u3611 \u3626 \u3660  (Steve Jobs) \u3595 \u3637 \u3629 \u3637 \u3650 \u3629 \u3649 \u3629 \u3611 \u3648 \u3611 \u3636 \u3621 \u3588 \u3629 \u3617 \u3614 \u3636 \u3623 \u3648 \u3605 \u3629 \u3619 \u3660 \u3648 \u3588 \u3618 \u3611 \u3619 \u3632 \u3585 \u3634 \u3624 \u3610 \u3609 \u3648 \u3623 \u3607 \u3637 \u3591 \u3634 \u3609  Macworld \u3607 \u3637 \u3656 \u3592 \u3633 \u3604 \u3586 \u3638 \u3657 \u3609 \u3651 \u3609 \u3595 \u3634 \u3609 \u3615 \u3619 \u3634 \u3609 \u3595 \u3636 \u3626 \u3650 \u3585 \u3648 \u3617 \u3639 \u3656 \u3629 \u3605 \u3657 \u3609 \u3611 \u3637 \u3607 \u3637 \u3656 \u3612 \u3656 \u3634 \u3609 \u3617 \u3634  \u3623 \u3656 \u3634 \u3649 \u3629 \u3611 \u3648 \u3611 \u3636 \u3621 \u3648 \u3611 \u3655 \u3609 \u3648 \u3592 \u3657 \u3634 \u3586 \u3629 \u3591 \u3621 \u3636 \u3586 \u3626 \u3636 \u3607 \u3608 \u3636 \u3660 \u3648 \u3607 \u3588 \u3650 \u3609 \u3650 \u3621 \u3618 \u3637 \u3609 \u3637 \u3657  \u3649 \u3621 \u3632 \u3592 \u3632 \u3609 \u3635 \u3617 \u3634 \u3651 \u3594 \u3657 \u3585 \u3633 \u3610 \u3650 \u3607 \u3619 \u3624 \u3633 \u3614 \u3607 \u3660 \u3617 \u3639 \u3629 \u3606 \u3639 \u3629 \u3619 \u3640 \u3656 \u3609 \u3621 \u3656 \u3634 \u3626 \u3640 \u3604  "\u3652 \u3629 \u3650 \u3615 \u3609  (iPhone)" \u3607 \u3637 \u3656 \u3617 \u3637 \u3588 \u3636 \u3623 \u3623 \u3634 \u3591 \u3592 \u3635 \u3627 \u3609 \u3656 \u3634 \u3618 \u3651 \u3609 \u3648 \u3604 \u3639 \u3629 \u3609 \u3627 \u3609 \u3657 \u3634 \
\
\uc0\u3652 \u3617 \u3656 \u3617 \u3637 \u3619 \u3634 \u3618 \u3591 \u3634 \u3609 \u3623 \u3656 \u3634 \u3652 \u3617 \u3650 \u3588 \u3619 \u3595 \u3629 \u3615 \u3607 \u3660 \u3652 \u3586 \u3586 \u3657 \u3629 \u3586 \u3657 \u3629 \u3591 \u3651 \u3592 \u3648 \u3619 \u3639 \u3656 \u3629 \u3591 \u3621 \u3636 \u3586 \u3626 \u3636 \u3607 \u3608 \u3636 \u3660 \u3648 \u3607 \u3588 \u3650 \u3609 \u3650 \u3621 \u3618 \u3637 \u3623 \u3656 \u3634  \u3629 \u3618 \u3656 \u3634 \u3591 \u3652 \u3619  \u3650 \u3604 \u3618 \u3652 \u3617 \u3650 \u3588 \u3619 \u3595 \u3629 \u3615 \u3607 \u3660 \u3648 \u3612 \u3618 \u3623 \u3656 \u3634 \u3652 \u3604 \u3657 \u3607 \u3635 \u3586 \u3657 \u3629 \u3605 \u3585 \u3621 \u3591 \u3585 \u3633 \u3610 \u3609 \u3634 \u3609 \u3634 \u3610 \u3619 \u3636 \u3625 \u3633 \u3607 \u3648 \u3614 \u3639 \u3656 \u3629 \u3609 \u3635 \u3649 \u3614 \u3621 \u3605 \u3615 \u3629 \u3619 \u3660 \u3617  Surface \u3652 \u3611 \u3651 \u3594 \u3657 \u3591 \u3634 \u3609 \u3592 \u3619 \u3636 \u3591 \u3649 \u3621 \u3657 \u3623  \u3652 \u3604 \u3657 \u3649 \u3585 \u3656 \u3607 \u3637 \u3650 \u3617 \u3610 \u3634 \u3618 \u3621 \u3660 \u3626 \u3634 \u3586 \u3634 \u3626 \u3627 \u3619 \u3633 \u3600  (T-Mobile USA) \u3605 \u3585 \u3621 \u3591 \u3609 \u3635 \u3648 \u3607 \u3588 \u3650 \u3609 \u3650 \u3621 \u3618 \u3637 \u3609 \u3637 \u3657 \u3652 \u3611 \u3651 \u3594 \u3657 \u3651 \u3609 \u3627 \u3609 \u3657 \u3634 \u3619 \u3657 \u3634 \u3609 \u3626 \u3634 \u3586 \u3634 \u3618 \u3656 \u3629 \u3618  \u3648 \u3614 \u3639 \u3656 \u3629 \u3626 \u3649 \u3585 \u3609 \u3649 \u3621 \u3632 \u3610 \u3633 \u3609 \u3607 \u3638 \u3585 \u3586 \u3657 \u3629 \u3617 \u3641 \u3621 \u3612 \u3621 \u3636 \u3605 \u3616 \u3633 \u3603 \u3601 \u3660 \u3607 \u3637 \u3656 \u3606 \u3641 \u3585 \u3626 \u3656 \u3591 \u3588 \u3639 \u3609  \u3619 \u3623 \u3617 \u3606 \u3638 \u3591 \u3649 \u3626 \u3604 \u3591 \u3586 \u3657 \u3629 \u3617 \u3641 \u3621 \u3619 \u3634 \u3588 \u3634 \u3586 \u3629 \u3591 \u3649 \u3605 \u3656 \u3621 \u3632 \u3610 \u3619 \u3636 \u3585 \u3634 \u3619 \u3607 \u3637 \u3656 \u3607 \u3637 \u3650 \u3617 \u3610 \u3634 \u3618 \u3621 \u3660 \u3617 \u3637 \u3629 \u3618 \u3641 \u3656  \u3627 \u3619 \u3639 \u3629 \u3650 \u3619 \u3591 \u3649 \u3619 \u3617 \u3651 \u3609 \u3648 \u3588 \u3619 \u3639 \u3629 \u3648 \u3594 \u3629 \u3619 \u3660 \u3619 \u3634 \u3605 \u3633 \u3609  (Sheraton) \u3607 \u3637 \u3656 \u3605 \u3585 \u3621 \u3591 \u3609 \u3635 \u3588 \u3629 \u3617 \u3614 \u3660 \u3650 \u3605 \u3658 \u3632 \u3585 \u3634 \u3649 \u3615 \u3609 \u3637 \u3657 \u3652 \u3611 \u3651 \u3594 \u3657 \u3651 \u3609 \u3621 \u3655 \u3629 \u3610 \u3610 \u3637 \u3657 \u3650 \u3619 \u3591 \u3649 \u3619 \u3617  \u3648 \u3614 \u3639 \u3656 \u3629 \u3651 \u3627 \u3657 \u3621 \u3641 \u3585 \u3588 \u3657 \u3634 \u3626 \u3634 \u3617 \u3634 \u3619 \u3606 \u3615 \u3633 \u3591 \u3648 \u3614 \u3621 \u3591  \u3626 \u3656 \u3591 \u3619 \u3641 \u3611 \u3616 \u3634 \u3614 \u3585 \u3621 \u3633 \u3610 \u3610 \u3657 \u3634 \u3609  \u3649 \u3621 \u3632 \u3626 \u3633 \u3656 \u3591 \u3595 \u3639 \u3657 \u3629 \u3627 \u3609 \u3633 \u3591 \u3626 \u3639 \u3629 \u3605 \u3634 \u3617 \u3605 \u3657 \u3629 \u3591 \u3585 \u3634 \u3619  \u3648 \u3594 \u3656 \u3609 \u3648 \u3604 \u3637 \u3618 \u3623 \u3585 \u3633 \u3610 \u3610 \u3656 \u3629 \u3609 \u3588 \u3634 \u3626 \u3636 \u3650 \u3609 \u3651 \u3609 \u3648 \u3588 \u3619 \u3639 \u3629 \u3630 \u3634 \u3619 \u3660 \u3619 \u3634 \u3627 \u3660  (Harrah) \u3649 \u3621 \u3632 \u3627 \u3621 \u3634 \u3618 \u3610 \u3619 \u3636 \u3625 \u3633 \u3607 \u3607 \u3637 \u3656 \u3592 \u3632 \u3609 \u3635 \u3648 \u3607 \u3588 \u3650 \u3609 \u3650 \u3621 \u3618 \u3637 \u3609 \u3637 \u3657 \u3652 \u3611 \u3651 \u3594 \u3657 \u3648 \u3614 \u3639 \u3656 \u3629 \u3585 \u3634 \u3619 \u3592 \u3635 \u3627 \u3609 \u3656 \u3634 \u3618 \u3605 \u3633 \u3659 \u3623 \u3594 \u3617 \u3585 \u3634 \u3619 \u3649 \u3626 \u3604 \u3591  \u3610 \u3619 \u3636 \u3585 \u3634 \u3619 \u3626 \u3611 \u3634  \u3649 \u3621 \u3632 \u3629 \u3639 \u3656 \u3609 \u3654 \
\
